{
  "name": "music-player",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "/usr/bin/env WEBPACK_PORT=5555 PORT=6161 ./node_modules/.bin/concurrently --raw npm:web-dev npm:server-dev",
    "web-dev-w": "webpack --watch --mode=development",
    "web-dev": "node scripts/webpack-dev-server.js",
    "cmd": "node -r ts-node/register src/server/music-player.ts",
    "rescan": "node -r ts-node/register src/server/music-player.ts rescan",
    "migrate": "node -r ts-node/register src/server/tasks/migrate.ts",
    "backup": "cp -v data/database.sqlite data/database-$(date +%F).sqlite",
    "server-dev": "nodemon -w 'src/server/**/*.*' ./node_modules/.bin/ts-node src/server/server.ts",
    "server": "node -r ts-node/register src/server/server.ts",
    "test": "/usr/bin/env DATABASE_URL=sqlite::memory: ./node_modules/.bin/mocha test/**/*.spec.ts"
  },
